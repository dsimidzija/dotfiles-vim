
snippet cons "console log"
console.log('`!v expand('%:t')`[`!v line('.')+1`]: ${1:var}${0}', $1);
endsnippet

snippet fun "function"
${1:name} = function (${2:params}) {
	${0}
	return;
}
endsnippet

snippet bfun "bound function"
${1:name} = (${2:params}) => {
	${0}
	return
}
endsnippet

snippet cb "callback"
function (${1:params}) {
	${0}
}
endsnippet

