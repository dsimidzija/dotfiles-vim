// @TODO: ${0}
endsnippet

snippet para "async.parallel"
parallel({
	${0}${1:first}: (next) => {
	},
	${2:second}: (next) => {
	}
}, (err, results) => {
	console.log('`!v expand('%:t')`[`!v line('.')+1`]: err, results', err, results);
});
endsnippet
